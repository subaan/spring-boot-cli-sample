<?xml version="1.0" encoding="UTF-8" ?>
<project name="maven-antrun-" default="main"  >
<target name="main">
  <taskdef resource="net/sf/antcontrib/antcontrib.properties"/>
  <taskdef classname="ise.antelope.tasks.StringUtilTask" name="stringutil"/>
  <var name="version-type" value="2.0.0.BUILD-SNAPSHOT"/>
  <propertyregex input="${version-type}" regexp=".*\.(.*)" property="version-type" replace="\1" override="true"/>
  <propertyregex input="${version-type}" regexp="(M)\d+" property="version-type" replace="MILESTONE" override="true"/>
  <propertyregex input="${version-type}" regexp="(RC)\d+" property="version-type" replace="MILESTONE" override="true"/>
  <propertyregex input="${version-type}" regexp="BUILD-(.*)" property="version-type" replace="SNAPSHOT" override="true"/>
  <stringutil string="${version-type}" property="repo">
    <lowercase/>
  </stringutil>
  <checksum file="/home/appranix/NetbeansProjects/spring-boot-applications/spring-boot-cli-sample/target/spring-boot-cli-2.0.0.BUILD-SNAPSHOT-bin.tar.gz" property="checksum" algorithm="sha-256"/>
  <echo message="Customizing homebrew for 2.0.0.BUILD-SNAPSHOT with checksum ${checksum} in ${repo} repo"/>
  <copy file="/home/appranix/NetbeansProjects/spring-boot-applications/spring-boot-cli-sample/src/main/homebrew/springboot.rb" tofile="/home/appranix/NetbeansProjects/spring-boot-applications/spring-boot-cli-sample/target/springboot.rb" overwrite="true">
    <filterchain>
      <expandproperties/>
    </filterchain>
  </copy>
  <attachartifact file="/home/appranix/NetbeansProjects/spring-boot-applications/spring-boot-cli-sample/target/springboot.rb" classifier="homebrew" type="rb"/>
</target>
</project>